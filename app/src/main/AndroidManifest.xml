<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mycalendar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Food_History" />
        <activity android:name=".Record_Food" />
        <activity android:name=".Sleep_History" />
        <activity android:name=".Record_Sleep" />
        <activity android:name=".Exercise_History" />
        <activity android:name=".Record_Exercise" />
        <activity android:name=".Record_Stress" />
        <activity android:name=".Stress_History" />
        <activity android:name=".Record_Cycle" />
        <activity android:name=".Cycle_History" />
        <activity android:name=".Status_History" />
        <activity android:name=".Update_Profile" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>